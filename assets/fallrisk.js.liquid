


$("img[alt^='https://cdn']").each(function( i ) {
  var vidContainer1 = "<video id='vid' autoplay='autoplay' loop='loop' muted='true' playsinline='' style='max-width: 28vw; margin: 0 auto; display: flex'> <source src='" ;
  var vidContainer2 = "' type='video/mp4'>Your browser does not support the video tag.</video>" ;
  var vidUrl = $(this).attr("alt");

  $(this).replaceWith( vidContainer1 + vidUrl + vidContainer2);
});

$( document ).ready(function() {

    $('span.cd_date').each(function(i, obj) {
      $( obj ).countdown({until: new Date(  obj.innerHTML.toString().replace(/\./g, ',') ), compact: true, layout: '{dn}{sep}{hnn}{sep}{mnn}{sep}{snn}',
       description: ''});
     });

  // document.getElementById('vid').play();


});
